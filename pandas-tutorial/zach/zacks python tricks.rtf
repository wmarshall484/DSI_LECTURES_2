def normalize_column(the_dataframe, col_to_normalize='', new_col_name='', how='zero_to_one'):\
    if how != 'zero_to_one':\
        df[new_col_name] = (the_dataframe[col_to_normalize] - the_dataframe[col_to_normalize].mean()) / (the_dataframe[col_to_normalize].max() - the_dataframe[col_to_normalize].min()) \
