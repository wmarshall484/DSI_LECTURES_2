def df_to_d3(df):\
    data = []\
    keys, labels = df.index, df.columns\
    for key in keys:\
        values = [\{'label': l, 'value': df[l][key]\} for l in labels]\
        data.append(\{'values': values, 'key': key\})\
    return data\
    \
def series_to_d3(s):\
    return [\{'label': k, 'value': v\} for k,v in s.to_dict().items()]\
