\f3 \cf21 \cb3 def\cf0  reset_game():\
    \cf21 global\cf0  current_room, white_room, red_room\
    white_room = \cf27 WhiteRoom\cf0 ()\
    red_room = \cf27 RedRoom\cf0 ()\
    current_room = white_room\
